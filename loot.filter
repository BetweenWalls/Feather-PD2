ItemDisplay[gps]:	%DARK_GREEN%= %GRAY%p³{%WHITE%Rancid Potion%NL%}%CONTINUE%	// Rancid
// Misc
ItemDisplay[yps]:	%DARK_GREEN%= %GRAY%A{%WHITE%Antidote Potion}%CONTINUE%		// Antidote
ItemDisplay[wms]:	%YELLOW%= %GRAY%t{%WHITE%Thawing Potion}%CONTINUE%		// Thawing
ItemDisplay[vps]:	%WHITE%= %GRAY%s{%WHITE%Stamina Potion}%CONTINUE%		// Stamina

// Hidden: Minor, Lesser, standard, & Greater potions - levels 18, 30, 40, 75
ItemDisplay[(hp1 OR mp1) CLVL>17]: {%NAME%}
ItemDisplay[(hp2 OR mp2) CLVL>29]: {%NAME%}
ItemDisplay[(hp3 OR mp3) CLVL>39]: {%NAME%}
ItemDisplay[(hp4 OR mp4) CLVL>74]: {%NAME%}

// Hidden: Throwing Potions - level 15
ItemDisplay[(opl OR opm OR ops OR gpl OR gpm OR gps) CLVL>14]: {%NAME%}

// Hidden: Stamina Potion, Thawing Potion - level 30
ItemDisplay[(wms OR vps) CLVL>29]: {%NAME%}

// Hidden: Antidote Potion - level 90
ItemDisplay[yps CLVL>89]: {%NAME%}


// +-------------------+
//-¦ Other Basic Items ¦-------------------------------------------------------------------------------------------------
// +-------------------+
// Hidden: Keys, Scrolls - level 20
ItemDisplay[key CLVL>19 !(ILVL=28 OR ILVL=64 OR ILVL=81 OR ILVL=79)]: {%WHITE%Key}	// except in Cow Levels and (Hell) Arcane Sanctuary
ItemDisplay[isc CLVL>19]: {%WHITE%Scroll of Identify}
ItemDisplay[tsc CLVL>19]: {%WHITE%Scroll of Town Portal}

// Modification: Quantity
ItemDisplay[QTY>1 !(WEAPON OR aqv OR cqv OR tbk OR ibk OR key OR RUNE>0)]: %NAME% %WHITE%×%QTY%%CONTINUE%

