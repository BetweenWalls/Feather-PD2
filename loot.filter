// Throwing
ItemDisplay[opl]:	%DARK_GREEN%= %GRAY%f¹{%WHITE%Fulminating Potion%NL%}%CONTINUE%	// Fulminating
ItemDisplay[opm]:	%DARK_GREEN%= %GRAY%f²{%WHITE%Exploding Potion%NL%}%CONTINUE%	// Exploding
